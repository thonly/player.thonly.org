    "copy1": "sudo cp /etc/letsencrypt/live/dns.thonly.org/fullchain.pem /workspaces/player.thonly.org/private",
    "copy2": "sudo cp /etc/letsencrypt/live/dns.thonly.org/privkey.pem /workspaces/player.thonly.org/private",
    "copy3": "sudo cp /etc/letsencrypt/live/dns.thonly.org/fullchain.pem /Users/siliconwat/Desktop/HeartBank®/THonly™/thonly.net/music.thonly.net/private",
